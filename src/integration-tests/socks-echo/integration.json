{
  "name": "fully proxying integration tests",
  "app": {
    "script": [
      "../../arraybuffers/arraybuffers.js",
      "../../handler/queue.js",
      "../../ipaddrjs/ipaddr.min.js",
      "../../logging/logging.js",
      "../../tcp/tcp.js",
      "../../webrtc/datachannel.js",
      "../../webrtc/peerconnection.js",
      "../../socks-common/socks-headers.js",
      "../../socks-to-rtc/socks-to-rtc.js",
      "../../rtc-to-net/rtc-to-net.js",
      "integration.js"
    ]
  },
  "provides": [
    "ProxyIntegrationTest"
  ],
  "default": "ProxyIntegrationTest",
  "api": {
    "ProxyIntegrationTest": {
      "singleEchoTest": {
        "type": "method",
        "value": ["buffer"],
        "ret": "buffer"
      },
      "parallelEchoTest": {
        "type": "method",
        "value": [["array", "buffer"]],
        "ret": ["array", "buffer"]
      },
      "serialEchoTest": {
        "type": "method",
        "value": [["array", "buffer"]],
        "ret": ["array", "buffer"]
      }
    }
  },
  "permissions": [
    "core.console",
    "core.rtcpeerconnection",
    "core.rtcdatachannel",
    "core.tcpsocket",
    "core.udpsocket"
  ]
}
