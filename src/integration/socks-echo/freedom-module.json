{
  "name": "fully proxying integration tests",
  "app": {
    "script": [
      "../../arraybuffers/arraybuffers.js",
      "../../handler/queue.js",
      "../../ipaddrjs/ipaddr.min.js",
      "../../logging/logging.js",
      "../../regex2dfa/regex2dfa.js",
      "../../tcp/tcp.js",
      "../../webrtc/datachannel.js",
      "../../webrtc/peerconnection.js",
      "../../churn/churn.js",
      "../../socks-common/socks-headers.js",
      "../../socks-to-rtc/socks-to-rtc.js",
      "../../rtc-to-net/rtc-to-net.js",
      "integration.js"
    ]
  },
  "dependencies": {
    "loggingcontroller": {
      "url": "../../loggingprovider/loggingprovider.json",
      "api": "loggingcontroller"
    },
    "churnPipe": {
      "url": "../../churn-pipe/freedom.json",
      "api": "churnPipe"
    }
  },
  "provides": [
    "ProxyIntegrationTest"
  ],
  "default": "ProxyIntegrationTest",
  "api": {
    "ProxyIntegrationTest": {
      "constructor": {
        "value": ["boolean", "boolean"]
      },
      "startEchoServer": {
        "type": "method",
        "value": [],
        "ret": "number"
      },
      "connect": {
        "type": "method",
        "value": ["number", "string"],
        "ret": "string"
      },
      "echo": {
        "type": "method",
        "value": ["string", "buffer"],
        "ret": "buffer"
      },
      "echoMultiple": {
        "type": "method",
        "value": ["string", ["array", "buffer"]],
        "ret": ["array", "buffer"]
      }
    }
  },
  "permissions": [
    "core.rtcpeerconnection",
    "core.rtcdatachannel",
    "core.tcpsocket",
    "core.udpsocket"
  ]
}
