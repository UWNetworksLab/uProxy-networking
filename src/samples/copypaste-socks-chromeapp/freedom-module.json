{
  "name": "socks-rtc-net",
  "description": "Sample socks over rtc to net Freedom manifest",
  "app": {
    "script": [
      "lib/arraybuffers/arraybuffers.js",
      "lib/handler/queue.js",
      "lib/logging/logging.js",
      "lib/tcp/tcp.js",
      "lib/webrtc/datachannel.js",
      "lib/webrtc/peerconnection.js",
      "lib/regex2dfa/regex2dfa.js",
      "lib/crypto/random.js",
      "lib/churn/churn.js",
      "lib/ipaddrjs/ipaddr.min.js",
      "lib/socks-common/socks-headers.js",
      "lib/socks-to-rtc/socks-to-rtc.js",
      "lib/rtc-to-net/rtc-to-net.js",
      "freedom-module.js"
    ]
  },
  "dependencies": {
    "churnPipe": {
      "url": "lib/churn-pipe/freedom.json",
      "api": "churnPipe"
    },
    "pgp": {
      "url": "lib/freedom-pgp-e2e/build/pgpapi.json",
      "api": "crypto"
    }
  },
  "permissions": [
    "core.console",
    "core.storage",
    "core.tcpsocket",
    "core.udpsocket",
    "core.rtcdatachannel",
    "core.rtcpeerconnection"
  ]
}
