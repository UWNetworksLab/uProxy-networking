{
  "manifest_version": 2,
  "name": "Echo-Server",
  "description": "Echo-Server Sample App",
  "version": "0.0.3",
  "app": {
    "background": {
      "scripts": [
        "lib/logging/logging.js",
        "lib/handler/queue.js",
        "lib/webrtc/third_party/adapter.js",
        "lib/webrtc/datachannel.js",
        "lib/webrtc/peerconnection.js",
        "background.js"
      ]
    }
  },
  "permissions": [
    { "socket": ["tcp-connect", "tcp-listen", "udp-bind", "udp-send-to"] }
  ]
}
