{
  "name": "TCP Echo Server",
  "description": "Listen for incoming requests, and echo them back.",
  "app": {
    "script": [
      "../arraybuffers/arraybuffers.js",
      "../handler/queue.js",
      "../logging/logging.js",
      "../tcp/tcp.js",
      "tcp-echo-server.js",
      "freedom-module.js"
    ]
  },
  "dependencies": {
    "loggingprovider": {
      "url": "../loggingprovider/loggingprovider.json",
      "api": "loggingprovider"
    }
  },
  "permissions": [
    "core.tcpsocket"
  ]
}
