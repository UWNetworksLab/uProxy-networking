{
  "manifest_version": 2,
  "name": "Simple SOCKS",
  "description": "Single-page SOCKS server.",
  "version": "0.0.2",
  "app": {
    "background": {
      "scripts": [
        "lib/freedom/uproxy-core-env.js",
        "background.js"
      ]
    }
  },
  "permissions": [],
  "sockets": {
    "tcp": {
      "connect" : ""
    },
    "tcpServer": {
      "listen": ""
    },
    "udp": {
      "bind": "*",
      "send": "*"
    }
  }
}
