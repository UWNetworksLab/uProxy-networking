{
  "name": "SOCKs to RTC peer",
  "description": "Listen for incoming requests, and relay them through a transport.",
  "app": {
    "script": [
      "../tcp/tcp.js",
      "../udp/udprelay.js",
      "../handler/handler-queue.js",
      "../arraybuffers/arraybuffers.js",
      "socks-headers.js",
      "socks.js",
      "socks-to-rtc.js"
    ]
  },
  "dependencies": {
    "transport": {
      "url": "../freedom-providers/transport.webrtc.json",
      "api": "transport"
    }
  },
  "permissions": [
    "core.tcpsocket",
    "core.udpsocket"
  ]
}
