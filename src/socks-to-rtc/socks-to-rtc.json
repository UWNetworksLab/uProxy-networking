{
  "name": "SOCKs to RTC peer",
  "description": "Listen for incoming requests, and relay them through a transport.",
  "app": {
    "script": [
      "tcp.js",
      "socks.js",
      "socks-headers.js",
      "socks-to-rtc.js",
      "udprelay.js",
      "tcp_echo_server.js",
      "../util/arraybuffers.js"
    ]
  },
  "dependencies": {
    "transport": {
      "url": "../freedom-providers/transport.webrtc.json",
      "api": "transport"
    }
  },
  "permissions": [
    "core.tcpsocket",
    "core.udpsocket"
  ]
}
