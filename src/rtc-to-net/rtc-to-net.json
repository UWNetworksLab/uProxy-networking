{
  "name": "RTC to Net peer",
  "description": "Proxy remote connections to their destination.",
  "app": {
    "script": [
      "netclient.js",
      "udpclient.js",
      "rtc-to-net.js",
      "../common/arraybuffers.js"
    ]
  },
  "dependencies": {
    "transport": {
      "url": "../freedom-providers/transport.webrtc.json",
      "api": "transport"
    }
  },
  "permissions": [
    "core.tcpsocket",
    "core.peerconnection",
    "core.udpsocket"
  ]
}
