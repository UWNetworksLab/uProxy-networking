{
  "name": "TURN backend",
  "description": "Remote endpoints for the TURN relay.",
  "app": {
    "script": [
      "turn-backend.js",
      "../logging/logging.js",
      "../turn-frontend/messages.js",
      "../arraybuffers/arraybuffers.js"
    ]
  },
  "provides": [
    "turnBackend"
  ],
  "api": {
    "turnBackend": {
      "handleIpc": {
        "type": "method",
        "value": [ "buffer" ]
      },
      "ipc": {
        "type": "event",
        "value": {
          "data": "buffer"
        }
      }
    }
  },
  "permissions": [
    "core.udpsocket",
    "core.log"
  ]
}
