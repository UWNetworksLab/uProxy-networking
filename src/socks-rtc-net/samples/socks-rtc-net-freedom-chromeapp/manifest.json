{
  "manifest_version": 2,
  "name": "socks-rtv-net",
  "description": "Sample app for proxying SOCKS5 over WebRTC",
  "version": "0.0.2",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    { "socket": ["tcp-connect", "tcp-listen", "udp-bind", "udp-send-to"] },
    "https://*.appspot.com/"
  ]
}
